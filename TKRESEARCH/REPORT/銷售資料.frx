      <TableDataSource Name="Table" DataType="System.Int32" Enabled="true" SelectCommand="--20221221 查SASLA&#13;&#10;&#13;&#10;SELECT  年月,LA006 AS '客代',MA002 AS '客戶',MR1MR003 AS '分類1',MR2MR003 AS '分類2',LA005 AS '品號',MB002 AS '品名',MB003 AS '規格',SUM(NUMS) AS '銷售數量',SUM(MMS) AS '銷售金額'&#13;&#10;FROM &#13;&#10;(&#13;&#10;SELECT LA006,SUBSTRING(LA015,1,6) AS '年月',MA002,MR1.MR003 MR1MR003,MR2.MR003 MR2MR003,LA007,LA015,LA005,MB002,MB003,NUMS,MMS&#13;&#10;FROM&#13;&#10;(&#13;&#10;SELECT LA006,LA007,CONVERT(NVARCHAR,LA015,112) LA015,LA005,(LA016-LA019) AS 'NUMS',(LA017-LA020-LA022-LA023) AS 'MMS'&#13;&#10;FROM [TK].dbo.SASLA&#13;&#10;WHERE 1=1&#13;&#10;AND ISNULL(LA006,'')&lt;&gt;'客戶空白'&#13;&#10;) AS TEMP&#13;&#10;LEFT JOIN [TK].dbo.COPMA ON MA001=LA006&#13;&#10;LEFT JOIN [TK].dbo.CMSMR MR1 ON MA017=MR1.MR002 AND MR1.MR001=1 &#13;&#10;LEFT JOIN [TK].dbo.CMSMR MR2 ON MA076=MR2.MR002 AND MR2.MR001=2 &#13;&#10;LEFT JOIN [TK].dbo.INVMB ON MB001=LA005&#13;&#10;WHERE (LA005 LIKE '4%' OR LA005 LIKE '5%')&#13;&#10;AND LA015&gt;='20221101' AND LA015&lt;='20221231'&#13;&#10;AND (MB001 LIKE '%410%' OR MB002 LIKE '%410%')&#13;&#10;) AS TEMP2&#13;&#10;GROUP BY LA006,MA002,MR1MR003,MR2MR003,LA007 ,LA005,MB002,MB003,年月">
